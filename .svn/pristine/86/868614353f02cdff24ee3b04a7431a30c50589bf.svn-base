package it.mef.bilancio.demdec.web.spring.form;

import it.almavivaitalia.web.sh.form.AbstractForm;

/**
 * @author Pietro
 *
 */
public class GestioneTipologiaDecretoForm extends AbstractForm {

	//tipologia decreto
	private String id;
	private String descTipologia;
	private String descSigla;
	private String descIter;

	private String selectedTipologia;
	private String tipoDecreto;
	private String modelloIter;


	/* getters & setters */
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getDescTipologia() {
		return descTipologia;
	}
	public void setDescTipologia(String descTipologia) {
		this.descTipologia = descTipologia;
	}
	public String getDescSigla() {
		return descSigla;
	}
	public void setDescSigla(String descSigla) {
		this.descSigla = descSigla;
	}
	public String getDescIter() {
		return descIter;
	}
	public void setDescIter(String descIter) {
		this.descIter = descIter;
	}
	public String getSelectedTipologia() {
		return selectedTipologia;
	}
	public void setSelectedTipologia(String selectedTipologia) {
		this.selectedTipologia = selectedTipologia;
	}
	public String getTipoDecreto() {
		return tipoDecreto;
	}
	public void setTipoDecreto(String tipoDecreto) {
		this.tipoDecreto = tipoDecreto;
	}
	public String getModelloIter() {
		return modelloIter;
	}
	public void setModelloIter(String modelloIter) {
		this.modelloIter = modelloIter;
	}

}
