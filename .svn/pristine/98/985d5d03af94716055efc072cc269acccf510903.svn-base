/* Classe container per centrare la griglia e limitare la larghezza */
.container {
 position: relative;
 width: 100%;
 max-width: 1000px;
 margin: 0 auto;
}

.container_field {
  border: 1px solid #A9BBCD;
  padding:7px 0px 3px 0px;
  margin-bottom: 10px;
}

