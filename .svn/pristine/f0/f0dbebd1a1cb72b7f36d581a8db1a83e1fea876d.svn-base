	private final static String securityMetricaLOCM1 = "LOCM"; 
	@SuppressWarnings("unused")
	private final static String securityMetricaLOCM2 = "LOCM";
	
	
	private String securityUtenteSB; 
	private String securityPswdSB; 
	private String securityUtente; 

	public LoginForm(){}
	
	
	public String getSecurityUtenteSB() {
		return securityUtenteSB;
	}
	public void setSecurityUtenteSB(String securityUtenteSB) {
		this.securityUtenteSB = securityUtenteSB;
