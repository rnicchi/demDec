package it.almavivaitalia.web.sh.export;

import it.almavivaitalia.web.sh.export.FieldFormatter;
public class HeaderCell{

	 
	
	private String title;
	private String beanPropertyName;
	private String beanPropertyDescName;
	public String getBeanPropertyDescName() {
		return beanPropertyDescName;
	}

	public void setBeanPropertyDescName(String beanPropertyDescName) {
		this.beanPropertyDescName = beanPropertyDescName;
	}

	private boolean key;

	public void setKey(boolean key) {
		this.key = key;
	}

	private FieldFormatter fieldFormatter;


	public FieldFormatter getFieldFormatter() {
		return fieldFormatter;
	}

	public void setFieldFormatter(FieldFormatter fieldFormatter) {
		this.fieldFormatter = fieldFormatter;
	}

	public String getBeanPropertyName() {
		return beanPropertyName;
	}

	public void setBeanPropertyName(String beanPropertyName) {
		this.beanPropertyName = beanPropertyName;
	}

	public String getTitle(){
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public boolean isKey() {
		return key;
	}

}
