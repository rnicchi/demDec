<script type="text/javascript">
    function apriPopUpModale(toOpen) {
        $('.overlay').fadeIn('fast');
        $('#' + toOpen).fadeIn('slow');
    }


    $(document).ready(function () {
        $(".apri").click(function () {
            apriPopUpModale();
        });

        $(".chiudi").click(
        function () {
            $('.overlay').fadeOut('fast');
            $('.box').hide();
        });

        //chiusura emergenza
        $(".overlay").click(
        function () {
            $(this).fadeOut('fast');
            $('.box').hide();
        });
    });

</script>






