package it.mef.bilancio.demdec.web.spring.form;

import it.almavivaitalia.web.sh.form.AbstractForm;

public class PredLettereTestiForm extends AbstractForm{
	
	private Long idLettera;
	private String sezione;
	private String numRiga;
	private String contenuto;
	
	
	public Long getIdLettera() {
		return idLettera;
	}
	public void setIdLettera(Long idLettera) {
		this.idLettera = idLettera;
	}
	public String getSezione() {
		return sezione;
	}
	public void setSezione(String sezione) {
		this.sezione = sezione;
	}
	public String getNumRiga() {
		return numRiga;
	}
	public void setNumRiga(String numRiga) {
		this.numRiga = numRiga;
	}
	public String getContenuto() {
		return contenuto;
	}
	public void setContenuto(String contenuto) {
		this.contenuto = contenuto;
	}
	

}
