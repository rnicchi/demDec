<!--   INIZIO GESTIONE MODALI --> 	
	<script type="text/javascript">
	    function apriPopUpModale(toOpen) {
	       
	        $('.overlay').fadeIn('fast');
	        $('#' + toOpen).fadeIn('slow');
	        return false;
	    }
	
	    function selectAll(){
	    	$('.checkPlut').prop('checked',true);
	    }
	    
	    function unselectAll(){
	    	$('.checkPlut').prop('checked',false);
	    }
	    
	    function close(){
	        $('.overlay').fadeOut('fast');
	        $('.box_pin').hide();
	        return false;
	        
	    }
	    
	    
	    $(document).ready(function () {
	        $(".apri").click(function () {
	            apriPopUpModale();
	        });
	
	        $(".chiudi").click(
	        function () {
	            $('.overlay').fadeOut('fast');
	            $('.box').hide();
	            $('.box_30').hide();
				$('.box_meta').hide();
				
	
	        });
	
	        //chiusura emergenza
	        $(".overlay").click(
	        function () {
	            $(this).fadeOut('fast');
	            $('.box').hide();
	            $('.box_30').hide();
				$('.box_meta').hide();
				$('.box_pin').hide();
	        });
	        
	         $(".chiudi_pin").click(
        	function () {
        	 close();
        	});
	        
	        $("#selectAll").click(function () {
	            selectAll();
	        });
	        
	        $("#unselectAll").click(function () {
	            unselectAll();
	        });
	        
	        $("#undo_pin").click(function () {
	            close();
	        });
	        
	        
	    });
	
	</script>
	
	
